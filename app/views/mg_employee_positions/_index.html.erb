<!-- Sec --> 
<button class="new-employee-position mg-btn-bottom-margin mg-custom-form-btn" id="newEmployeePositionID">New Profile</button>
<h4 class="mg-h4">Listing Profile</h4>
<% if @employee_positions.length > 0%>
<div class="mg_pagination">
    <table class="batch-tbl mg-tbl-margin" id="EmpCatTableID">
        <tr>
          <th><%= t('employeecategory') %></th>
          <th><%= t('profilename') %></th>
          <!-- <th><%#= t('status') %></th> -->
          <th><%= t('actions') %></th>
        </tr>
       
        <% @employee_positions.each do |employee_position| %>
           <%= render partial: 'table_row', locals: {employee_position: employee_position} %>
        <% end %>
    </table>
     <%= will_paginate @employee_positions, :container => false %>
    </div>
<% else %>
  <h4>Please create some employee position... </h4>

<% end %>
<div id="newEmployeePositionDialogID" ></div>