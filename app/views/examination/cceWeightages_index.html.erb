<!-- breadcrumbs Work -->
<% navigation_add 'Examinations', examinations_path %>
<% navigation_add 'CCE', examination_cce_new_path %>
<% navigation_add 'Basic Settings', examination_cceBasic_Settings_path %>
<% navigation_add 'CCE Exam Weightages', examination_cceWeightages_index_path %>

<!-- breadcrumbs Work -->




<%=render  partial: 'examination_table_left' %>
<h4 class="mg-h4">CCE Exam Weightages</h4>
<div>
  <button class="cce-Weightages-datas mg-custom-form-btn" id="<%= @id%>">New </button><br/>


  <div id="cce_Weightages">
  </div>
  <div id="cce_Weightages_edit">
  </div>


<table class="batch-tbl mg-tbl-margin" cellspacing="0" style="padding:0; border-collapse:collapse;">

 
<%if  @cceWeightage[0].to_s.empty? %>
<h5><%="No Records Found"%></h5>

<%else%>
  <tr>
    <th>Weightages</th>
    <th>CCE Exam Category</th>
    <th><%=t('actions')%></th>
  </tr>
  <% @cceWeightage.each do |data| %>
    <tr>
   
      <td><%= data.weightages %>
        <% @data=data.criteria_type %>
        <%="(#{@data})"%>
      </td>

      <%@id=data.mg_cce_exam_category_id%>

      <%@categorydata=MgCceExamCategory.find(data.mg_cce_exam_category_id)%>




      <td><%= @categorydata.name %></td>

   


      <td>
        <a title="Edit" href="#" id="<%=raw data.id%>" class="examCategory-edit-class"><button class="mg-text-exam mg-icon-btn"><i class="fa fa-pencil-square-o"></i></button></a>
        <a title="Delete" href="#" id="<%=raw data.id%>" class="examCategory-delete-class"><button class="mg-text-exam mg-icon-btn"><i class="fa fa-trash"></i></button></a>
      </td>
    </tr>
  <%end %>
  <%end%>
</table> 
</div>
<%= render  partial: 'examination_table_close' %><br/>


<script type="text/javascript">
  $(document).ready(function() {
        focusCurrentTab("viewStudentListAId","viewStudentListLiId");
        focusExaminationHeaderBox();
  });

</script>