<%= render  partial: 'examination_table_left'%>

<!-- <h1><%#= "Exam" %></h1> -->
<h4 class="mg-h4"><%= "Student Ranking Per Batch" %></h4>
  
 
<div class="mg-custom-labels">
  <div id="page-yield">
    <% unless flash[:notice].nil? %>
      <h5 class="flash-msg"> <%= flash[:notice] %> </h5>
    <% end %>
    <div class="box">

      <%= form_for :batchRanking, :url=>{:action=>'batch_wise_student_rank'} do |f| %>
        <div class="label-field-pair">
          <label class="mg-label" for="student_course"><%= t('select_a_batch') %>:</label>
          <div  class="text-input-bg">
            <%=select_tag :mg_batch_id,options_from_collection_for_select(@batches,"id","full_name"),:prompt=>"All Batches"%></div></div><br>
        <%= f.submit t('save') ,:class=>"mg-small-form-btn"%>
      <% end %>
      <div class="extender"></div>
    </div></div>
</div>

   <%= render  partial: 'examination_table_close' %>