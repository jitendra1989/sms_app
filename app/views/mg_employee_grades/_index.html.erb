<button class="new-employee-grade mg-btn-bottom-margin mg-icon-btn" id="newEmployeeGradeID"><%=t('create_new_grade')%></button>
<h4 class="mg-h4"><%=t('grade_list')%></h4>
<% if @employee_grades.length > 0 %>
<div class="mg_pagination">
    <table class="batch-tbl" id="EmpGraTableID">
        <tr>
          <th><%= t('gradename') %></th>
          <th><%= t('actions') %></th>
        </tr>
       
        <% @employee_grades.each do |employee_grade| %>
           <%= render partial: 'table_row', locals: {employee_grade: employee_grade} %>
        <% end %>
    </table><br/>
    <%= will_paginate @employee_grades, :container => false %>
    </div>
<% else %>
  <h5>Please create some employee grades</h5>

<% end %>



<div id="editEmployeeGradesDialogID"></div>
<div id="newEmployeeGradesDialogID"></div>
<div id="showEmployeeGradesDialogID"></div>


