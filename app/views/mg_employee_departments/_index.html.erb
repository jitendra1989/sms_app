<!-- Sec --> 
<button class="new-employee-department mg-btn-bottom-margin mg-custom-form-btn" id="newEmployeeDepartmentID">New Department</button>
<h4 class="mg-h4">Listing Department</h4>
<% if @employee_departments.length > 0 %>
<div class="mg_pagination">
    <table class="batch-tbl mg-tbl-margin" id="EmpCatTableID">
        <tr>
          <th><%= t('departmentname') %></th>
          <th><%= t('departmentcode') %></th>
          <th><%= t('actions') %></th>
        </tr>
       
        <% @employee_departments.each do |employee_department| %>
           <%= render partial: 'table_row', locals: {employee_department: employee_department} %>
        <% end %>
    </table><br/>
     <%= will_paginate @employee_departments, :container => false %>
    </div>
<% else %>
  <h4>Please create some employee departments... </h4>

<% end %>
<div id="newEmployeeDepartmentDialogID" ></div>
<div id="editEmployeeDepartmentDialogID"></div>
